{
    "name": "renatomiguelsantos/imagify-laravel",
    "description": "Laravel integration for the imagify.io API",
    "keywords": ["imagify", "laravel", "images", "compression"],
    "license": "MIT",
    "authors": [
        {
            "name": "Renato Santos",
            "email": "renato.miguel@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "wp-media/imagify-php": "^1.1"
    },
    "autoload": {
        "psr-4": { "Imagify\\Laravel\\": "src/" }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Imagify\\Laravel\\ImagifyServiceProvider"
            ],
            "aliases": {
                "Imagify": "Imagify\\Laravel\\Facades\\Imagify"
            }
        }
    }
}
